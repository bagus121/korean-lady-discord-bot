{
	"extends": "eslint:recommended",
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2019
	},
	"rules": {
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"no-unreachable": "warn",
		"no-unused-vars": "off",
		"constructor-super": "warn",
		"valid-typeof": "warn",
		"func-names": "off",
		"curly": "off",
		"vars-on-top": "off",
		"no-inner-declarations": "off"
	}
}
